{
  "version": 4,
  "configurations": [
    {
      "name": "Mingw64",
      "compilerPath": "C:/msys64/mingw64/bin/g++.exe",
      "intelliSenseMode": "windows-gcc-x64",
      "includePath": [
        "${workspaceFolder}/src",
        "${workspaceFolder}/src/**",
        "C:/msys64/mingw64/include",
        "C:/msys64/mingw64/include/**"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/src",
          "C:/msys64/mingw64/include",
          "C:/msys64/mingw64/include/**"
        ],
        "limitSymbolsToIncludedHeaders": true
      },
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "cStandard": "c17",
      "cppStandard": "gnu++17"
    }
  ]
}
